// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model listofproducts {
  id                    String    @id
  merchantid            String
  productcatalogueid    String
  name                  String
  price                 Float
  tax                   Float
  description           String?
  discount              Float?
  brand                 String?
}